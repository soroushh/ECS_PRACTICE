{
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "947341666543.dkr.ecr.eu-west-1.amazonaws.com/nginx:PROD_%COMMIT%",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": [
        "app"
      ]
    },
    {
      "name": "app",
      "image": "947341666543.dkr.ecr.eu-west-1.amazonaws.com/flaskapp:PROD_%COMMIT%",
      "essential": true,
      "command": ["gunicorn3", "-b", "0.0.0.0:8000", "app:app", "--workers=5"]
    }
  ],
  "networkMode": "bridge",
  "family": "ECS_PRACTING_TASK",
  "requiresCompatibilities": ["EC2"],
  "cpu": "256",
  "memory": "256"
}
