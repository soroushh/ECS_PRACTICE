{
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "947341666543.dkr.ecr.eu-west-1.amazonaws.com/nginx:PROD_$(CI_COMMIT_ID)",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "cpu": 256,
      "memory": 256,
      "links": [
        "flaskapp"
      ]
    },
    {
      "name": "flaskapp",
      "image": "947341666543.dkr.ecr.eu-west-1.amazonaws.com/flaskapp:PROD_$(CI_COMMIT_ID)",
      "essential": true,
      "cpu": 256,
      "memory": 256
    }
  ],
  "networkMode": "bridge",
  "family": "ECS_PRACTING_TASK",
  "requiresCompatibilities": ["EC2"]
}
